{
  "name": "kaualandi-new",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "build": "ng build && node compress.js",
    "build:prod": "ng build --configuration production && node compress.js",
    "compress": "node compress.js",
    "prepare": "husky",
    "commit": "git-cz",
    "lint": "ng lint",
    "clear": "rm -rf node_modules && rm package-lock.json && rm -rf .angular && npm install",
    "s3": "aws s3 cp dist/browser s3://goat-web-dev --recursive",
    "s3:prod": "aws s3 cp dist/browser s3://goat-web-dev --recursive",
    "build:s3": "ng build && npm run s3",
    "build:prod:s3": "ng build --configuration production && npm run s3:prod",
    "cloudfront": "aws cloudfront create-invalidation --distribution-id E1XNITORYSVKIE --paths '/*'",
    "post-release": "ts-node ./version.ts && git c \"chore(env): update enviroment versions\" && git push --tags",
    "release": "standard-version",
    "release:ma": "standard-version --release-as major && npm run post-release",
    "release:mi": "standard-version --release-as minor && npm run post-release",
    "release:p": "standard-version --release-as patch && npm run post-release",
    "release:first": "standard-version --first-release && npm run post-release",
    "serve:ssr:kaualandi-new": "node dist/server/server.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/cdk": "^18.2.14",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/material": "^18.2.14",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/platform-server": "^18.2.0",
    "@angular/pwa": "^0.5.3",
    "@angular/router": "^18.2.0",
    "@angular/ssr": "^18.2.12",
    "@ngx-translate/core": "^16.0.3",
    "@ngx-translate/http-loader": "^16.0.0",
    "express": "^4.18.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.12",
    "@angular-eslint/builder": "19.0.2",
    "@angular-eslint/eslint-plugin": "19.0.2",
    "@angular-eslint/eslint-plugin-template": "19.0.2",
    "@angular-eslint/schematics": "19.0.2",
    "@angular-eslint/template-parser": "19.0.2",
    "@angular/cli": "^18.2.12",
    "@angular/compiler-cli": "^18.2.0",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "@typescript-eslint/parser": "7.11.0",
    "autoprefixer": "^10.4.20",
    "browserslist": "^4.24.3",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.4.49",
    "prettier": "3.4.2",
    "prettier-eslint": "^16.3.0",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.4.16",
    "ts-node": "^10.9.2",
    "typescript": "~5.5.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
